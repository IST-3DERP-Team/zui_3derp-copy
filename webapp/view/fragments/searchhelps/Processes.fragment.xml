		confirm="_processesValueHelpClose"
		cancel="_processesValueHelpClose">
		<StandardListItem title="{ProcessCodeModel>ProcessCd}" description="{ProcessCodeModel>Desc1}"/>
	</SelectDialog>
</core:FragmentDefinition>