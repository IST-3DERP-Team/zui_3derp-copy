<core:FragmentDefinition
	xmlns="sap.m"
	xmlns:core="sap.ui.core">
	<SelectDialog
		title="{i18n>SelectProcessCode}"
		class="sapUiPopupWithPadding"
		items="{ProcessCodeModel>/results}"
